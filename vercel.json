{
  "version": 2,
  "builds": [
    {
      "src": "client/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "client/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "client/*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/(?!.*\\.(js|css|png|jpg|jpeg|gif|svg)$)(.*)$",
      "dest": "/client/index.html"
    }
  ]
}
